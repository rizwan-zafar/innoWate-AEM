<?xml version="1.0" encoding="UTF-8"?>
<jcr:root xmlns:sling="http://sling.apache.org/jcr/sling/1.0" xmlns:granite="http://www.adobe.com/jcr/granite/1.0" xmlns:cq="http://www.day.com/jcr/cq/1.0" xmlns:jcr="http://www.jcp.org/jcr/1.0" xmlns:nt="http://www.jcp.org/jcr/nt/1.0"
    jcr:primaryType="nt:unstructured"
    jcr:title="Title"
    sling:resourceType="cq/gui/components/authoring/dialog"
    helpPath="https://www.adobe.com/go/aem_cmp_title_v1"
    trackingFeature="core-components:title:v1">
    <content
        jcr:primaryType="nt:unstructured"
        sling:resourceType="granite/ui/components/coral/foundation/fixedcolumns">
        <items jcr:primaryType="nt:unstructured">
            <column
                jcr:primaryType="nt:unstructured"
                sling:resourceType="granite/ui/components/coral/foundation/container">
                <items jcr:primaryType="nt:unstructured">
                    <tabs
                        jcr:primaryType="nt:unstructured"
                        sling:resourceType="granite/ui/components/coral/foundation/tabs"
                        maximized="{Boolean}true">
                        <items jcr:primaryType="nt:unstructured">
                            <Details
                                jcr:primaryType="nt:unstructured"
                                jcr:title="Details"
                                sling:resourceType="granite/ui/components/coral/foundation/container"
                                margin="{Boolean}true">
                                <items jcr:primaryType="nt:unstructured">
                                    <title
                                        jcr:primaryType="nt:unstructured"
                                        sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                        fieldDescription="The text to display."
                                        fieldLabel="Title"
                                        name="./title"/>
                                    <description
                                        jcr:primaryType="nt:unstructured"
                                        sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                        fieldDescription="The text to display."
                                        fieldLabel="Description"
                                        name="./description"/>
                                </items>
                            </Details>
                            <usefulllinks
                                jcr:primaryType="nt:unstructured"
                                jcr:title="Use Full Links"
                                sling:resourceType="granite/ui/components/coral/foundation/container"
                                margin="{Boolean}true">
                                <items jcr:primaryType="nt:unstructured">
                                    <usefullLink
                                        granite:class="dialog__spacer--bottom"
                                        jcr:primaryType="nt:unstructured"
                                        sling:resourceType="granite/ui/components/coral/foundation/form/multifield"
                                        composite="{Boolean}true"
                                        fieldLabel="Items"
                                        name="./usefullLink">
                                        <field
                                            jcr:primaryType="nt:unstructured"
                                            sling:resourceType="granite/ui/components/coral/foundation/container"
                                            name="./usefulllink">
                                            <items jcr:primaryType="nt:unstructured">
                                                <Label
                                                    jcr:primaryType="nt:unstructured"
                                                    sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                                    fieldDescription="The text to display."
                                                    fieldLabel="Label"
                                                    name="./label"/>
                                                <link
                                                    jcr:primaryType="nt:unstructured"
                                                    sling:resourceType="granite/ui/components/coral/foundation/form/pathfield"
                                                    fieldLabel="Link"
                                                    name="./link"
                                                    placeholder=""/>
                                            </items>
                                        </field>
                                    </usefullLink>
                                </items>
                            </usefulllinks>
                            <services
                                jcr:primaryType="nt:unstructured"
                                jcr:title="Services"
                                sling:resourceType="granite/ui/components/coral/foundation/container"
                                margin="{Boolean}true">
                                <items jcr:primaryType="nt:unstructured">
                                    <services
                                        granite:class="dialog__spacer--bottom"
                                        jcr:primaryType="nt:unstructured"
                                        sling:resourceType="granite/ui/components/coral/foundation/form/multifield"
                                        composite="{Boolean}true"
                                        fieldLabel="Items"
                                        name="./services">
                                        <field
                                            jcr:primaryType="nt:unstructured"
                                            sling:resourceType="granite/ui/components/coral/foundation/container"
                                            name="./services">
                                            <items jcr:primaryType="nt:unstructured">
                                                <Label
                                                    jcr:primaryType="nt:unstructured"
                                                    sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                                    fieldDescription="The text to display."
                                                    fieldLabel="Label"
                                                    name="./label"/>
                                                <link
                                                    jcr:primaryType="nt:unstructured"
                                                    sling:resourceType="granite/ui/components/coral/foundation/form/pathfield"
                                                    fieldLabel="Link"
                                                    name="./link"
                                                    placeholder=""/>
                                            </items>
                                        </field>
                                    </services>
                                </items>
                            </services>
                            <Contact
                                jcr:primaryType="nt:unstructured"
                                jcr:title="Contact Us"
                                sling:resourceType="granite/ui/components/coral/foundation/container"
                                margin="{Boolean}true">
                                <items jcr:primaryType="nt:unstructured">
                                    <address
                                        jcr:primaryType="nt:unstructured"
                                        sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                        fieldDescription="The text to display."
                                        fieldLabel="Address"
                                        name="./address"/>
                                    <phone
                                        jcr:primaryType="nt:unstructured"
                                        sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                        fieldDescription="The text to display."
                                        fieldLabel="Phone"
                                        name="./phone"/>
                                    <email
                                        jcr:primaryType="nt:unstructured"
                                        sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                        fieldDescription="The text to display."
                                        fieldLabel="Email"
                                        name="./email"/>
                                </items>
                            </Contact>
                            <CopyRight
                                jcr:primaryType="nt:unstructured"
                                jcr:title="Copy Right"
                                sling:resourceType="granite/ui/components/coral/foundation/container"
                                margin="{Boolean}true">
                                <items jcr:primaryType="nt:unstructured">
                                    <copyright
                                        jcr:primaryType="nt:unstructured"
                                        sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                        fieldDescription="The text to display."
                                        fieldLabel="Copy Right Text"
                                        name="./copyright"/>
                                    <madeby
                                        jcr:primaryType="nt:unstructured"
                                        sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                        fieldDescription="The text to display."
                                        fieldLabel="Made By"
                                        name="./madeby"/>
                                </items>
                            </CopyRight>
                        </items>
                    </tabs>
                </items>
            </column>
        </items>
    </content>
</jcr:root>

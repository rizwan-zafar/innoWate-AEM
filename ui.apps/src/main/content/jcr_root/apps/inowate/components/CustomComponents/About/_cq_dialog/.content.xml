<?xml version="1.0" encoding="UTF-8"?>
<jcr:root xmlns:sling="http://sling.apache.org/jcr/sling/1.0" xmlns:cq="http://www.day.com/jcr/cq/1.0" xmlns:jcr="http://www.jcp.org/jcr/1.0" xmlns:nt="http://www.jcp.org/jcr/nt/1.0"
    jcr:primaryType="nt:unstructured"
    jcr:title="Title"
    sling:resourceType="cq/gui/components/authoring/dialog"
    helpPath="https://www.adobe.com/go/aem_cmp_title_v1"
    trackingFeature="core-components:title:v1">
    <content
        jcr:primaryType="nt:unstructured"
        sling:resourceType="granite/ui/components/coral/foundation/fixedcolumns">
        <items jcr:primaryType="nt:unstructured">
            <column
                jcr:primaryType="nt:unstructured"
                sling:resourceType="granite/ui/components/coral/foundation/container">
                <items jcr:primaryType="nt:unstructured">
                    <subHeading
                        jcr:primaryType="nt:unstructured"
                        sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                        fieldDescription="subHeading"
                        fieldLabel="Sub Heading"
                        name="./subHeading"/>
                    <title
                        jcr:primaryType="nt:unstructured"
                        sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                        fieldDescription="Leave empty to use the page title."
                        fieldLabel="Title"
                        name="./title"/>
                    <description
                        jcr:primaryType="nt:unstructured"
                        sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                        fieldDescription="Leave empty to use the page title."
                        fieldLabel="Description"
                        name="./description"/>
                    <btnText
                        jcr:primaryType="nt:unstructured"
                        sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                        fieldDescription="Add text of button"
                        fieldLabel="Button Text"
                        name="./btnText"/>
                    <cardOneTitle
                        jcr:primaryType="nt:unstructured"
                        sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                        fieldDescription="cardOneTitle"
                        fieldLabel="Card One Title"
                        name="./cardOneTitle"/>
                    <cardOneDescription
                        jcr:primaryType="nt:unstructured"
                        sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                        fieldDescription="cardOneDescription"
                        fieldLabel="Card One Description"
                        name="./cardOneDescription"/>
                    <cardTwoTitle
                        jcr:primaryType="nt:unstructured"
                        sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                        fieldDescription="cardTwoTitle"
                        fieldLabel="Card Two Title"
                        name="./cardTwoTitle"/>
                    <cardTwoDescription
                        jcr:primaryType="nt:unstructured"
                        sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                        fieldDescription="cardTwoDescription"
                        fieldLabel="Card Two Description"
                        name="./cardTwoDescription"/>
                    <cardThreeTitle
                        jcr:primaryType="nt:unstructured"
                        sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                        fieldDescription="cardThreeTitle"
                        fieldLabel="Card Three Title"
                        name="./cardThreeTitle"/>
                    <cardThreeDescription
                        jcr:primaryType="nt:unstructured"
                        sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                        fieldDescription="cardThreeDescription"
                        fieldLabel="Card Three Description"
                        name="./cardThreeDescription"/>
                    <cardFourTitle
                        jcr:primaryType="nt:unstructured"
                        sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                        fieldDescription="cardFourTitle"
                        fieldLabel="Card Four Title"
                        name="./cardFourTitle"/>
                    <cardFourDescription
                        jcr:primaryType="nt:unstructured"
                        sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                        fieldDescription="cardFourDescription"
                        fieldLabel="Card Four Description"
                        name="./cardFourDescription"/>
                </items>
            </column>
        </items>
    </content>
</jcr:root>
